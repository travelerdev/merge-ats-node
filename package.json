{
  "name": "@travelerdev/merge-ats-node",
  "version": "0.1.2",
  "license": "MIT",
  "type": "module",
  "platform": "node",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./*": "./dist/*/index.js"
  },
  "tsup": {
    "external": [
      "axios",
      "form-data"
    ],
    "dts": true,
    "format": [
      "cjs",
      "esm"
    ],
    "entryPoints": [
      "src/index.ts"
    ],
    "splitting": false,
    "minify": true,
    "sourcemap": true,
    "clean": true,
    "target": "node14"
  },
  "scripts": {
    "pregenerate": "rm -rf ./src/generated",
    "generate": "npx openapi-typescript-codegen --input ./openapi-spec.json --output ./src/generated --useOptions --client axios --name UnderlyingMergeATS",
    "postgenerate": "eslint src/generated --fix",
    "build": "tsup src/*",
    "test": "jest",
    "prepublishOnly": "yarn test",
    "prepare": "yarn build",
    "lint": "eslint src --fix"
  },
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "axios": "^1.1.0",
    "eslint": "^8.27.0",
    "form-data": "^4.0.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "axios": "^0.27.0|^1.0.0|^1.1.0",
    "form-data": ">=4.0.0 <5.0.0"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.m?[tj]sx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    }
  }
}
