{
  "name": "@travelerdev/merge-ats-node",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "platform": "node",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./index.cjs",
      "import": "./index.js"
    }
  },
  "tsup": {
    "external": [
      "axios",
      "form-data"
    ],
    "dts": true,
    "format": [
      "cjs",
      "esm"
    ],
    "entryPoints": [
      "src/index.ts"
    ],
    "splitting": false,
    "minify": true,
    "sourcemap": true,
    "clean": true,
    "target": "node14"
  },
  "scripts": {
    "pregenerate": "rm -rf ./src/generated",
    "generate": "npx openapi-typescript-codegen --input ./openapi-spec.json --output ./src/generated --useOptions --client axios --name UnderlyingMergeATS",
    "postgenerate": "eslint src/generated --fix",
    "build": "tsup src/*",
    "test": "ava",
    "pretest": "yarn build",
    "prepublishOnly": "yarn test",
    "lint": "eslint src --fix"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "ava": "^5.1.0",
    "axios": "^1.1.0",
    "eslint": "^8.27.0",
    "form-data": "^4.0.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "axios": ">1.0.0",
    "form-data": ">=4.0.0 <5.0.0"
  }
}
